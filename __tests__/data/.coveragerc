[run]
source = .
omit = 
    */tests/*
    */__init__.py
    */site-packages/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[html]
directory = htmlcov
title = My Project Coverage Report

[xml]
output = coverage.xml

[paths]
source =
    src/
    lib/
    .

[coverage:config]
branch = True 